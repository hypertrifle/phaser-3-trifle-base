{
  "name": "hypertrifle-phaser-tempalte",
  "version": "1.1.0",
  "description": "A Phaser 3 Typescript Setup with useful common funtionallity",
  "main": "src/index.js",
  "scripts": {
    "help": "node scripts/help.js",
    "dev": "webpack-dev-server --mode development --open-page webpack-dev-server/build/ --hot --inline --config webpack.common.config.js",
    "build": "webpack --config webpack.common.config.js --mode production",
    "build-ci": "webpack --config webpack.common.config.js --mode production --env.NODE_ENV=ci",
    "lint": "eslint --fix src/**/*.ts",
    "docs": "typedoc --includeDeclarations --includes ./phaser.d.ts --ignoreCompilerErrors --excludeExternals --out ./docs/ ./src/ && http-server docs -c-1 -o"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:/hypertrifle/phaser-3-trifle-base.git"
  },
  "author": "Rich Searle - hypertrifle.com",
  "license": "UNLICENCED",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "@types/dat.gui": "^0.7.5",
    "@types/node": "^12.12.14",
    "@types/stats.js": "^0.17.0",
    "@types/webfontloader": "^1.6.29",
    "@typescript-eslint/parser": "^2.10.0",
    "babel-loader": "^8.0.6",
    "dat.gui": "^0.7.6",
    "file-loader": "^5.0.2",
    "strip-comments": "^2.0.1",
    "ts-loader": "^5.4.5",
    "typescript-json-schema": "^0.34.0",
    "webpack": "^4.41.2",
    "webpack-dev-server": "^3.9.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.7.0",
    "@typescript-eslint/eslint-plugin": "^2.10.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.2.1",
    "eslint": "^6.7.2",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-hsv2rgb": "^1.0.0",
    "glsl-noise": "0.0.0",
    "glslify": "^7.0.0",
    "glslify-loader": "^2.0.0",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.13.0",
    "phaser": "^3.21.0",
    "raw-loader": "^0.5.1",
    "sass-loader": "^8.0.0",
    "strip-json-comments": "^2.0.1",
    "style-loader": "^1.0.1",
    "terser-webpack-plugin": "^1.4.1",
    "typescript": "^2.9.2",
    "typescript-eslint": "0.0.1-alpha.0",
    "webfontloader": "^1.6.28",
    "webpack-cli": "^3.3.10"
