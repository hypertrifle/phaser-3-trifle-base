  "license": "UNLICENCED",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/preset-env": "^7.10.2",
    "@types/dat.gui": "^0.7.5",
    "@types/node": "^14.0.13",
    "@types/stats.js": "^0.17.0",
    "@types/webfontloader": "^1.6.32",
    "@typescript-eslint/parser": "^3.3.0",
    "babel-loader": "^8.1.0",
    "dat.gui": "^0.7.7",
    "file-loader": "^6.0.0",
    "sass": "^1.26.8",
    "strip-comments": "^2.0.1",
    "ts-loader": "^7.0.5",
