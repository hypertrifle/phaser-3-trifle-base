{
  "name": "hypertrifle-phaser-tempalte",
  "version": "1.1.0",
  "description": "A Phaser 3 Typescript Setup with useful common funtionallity",
  "main": "src/index.js",
  "scripts": {
    "help": "node scripts/help.js",
    "dev": "webpack-dev-server --mode development --open-page webpack-dev-server/ --hot --inline --host 127.0.0.1  --config webpack.common.config.js",
    "build": "webpack --config webpack.common.config.js --mode production",
    "build:native": "npx cap copy",
    "lint": "eslint --fix src/**/*.ts",
    "docs": "typedoc --includeDeclarations --includes ./phaser.d.ts --ignoreCompilerErrors --excludeExternals --out ./docs/ ./src/ && http-server docs -c-1 -o",
    "json-export": "ts-node scripts/generateJSON.ts"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:/hypertrifle/phaser-3-trifle-base.git"
  },
  "author": "Rich Searle - hypertrifle.com",
  "license": "UNLICENCED",
  "private": true,
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@types/dat.gui": "^0.7.5",
    "@types/node": "^14.6.0",
    "@types/stats.js": "^0.17.0",
    "@types/webfontloader": "^1.6.32",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/parser": "^3.9.1",
    "babel-loader": "^8.1.0",
    "dat.gui": "^0.7.7",
    "file-loader": "^6.0.0",
    "sass": "^1.26.10",
    "strip-comments": "^2.0.1",
    "ts-loader": "^8.0.7",
    "typescript-json-schema": "^0.43.0",
    "webpack": "^4.44.1",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.10.4",
    "@capacitor/android": "^2.4.0",
    "@capacitor/cli": "^2.4.0",
    "@capacitor/core": "^2.4.0",
    "@capacitor/ios": "^2.4.0",
    "@typescript-eslint/eslint-plugin": "^3.9.1",
    "@typescript-eslint/typescript-estree": "^3.9.1",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^4.2.1",
    "eslint": "^7.7.0",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-hsv2rgb": "^1.0.0",
    "glsl-noise": "0.0.0",
    "glslify": "^7.1.0",
    "glslify-loader": "^2.0.0",
