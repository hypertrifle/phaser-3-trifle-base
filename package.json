    "@capacitor/ios": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^3.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.2.0",
    "glsl-hsl2rgb": "^1.1.0",
    "glsl-hsv2rgb": "^1.0.0",
    "glsl-noise": "0.0.0",
    "glslify": "^7.0.0",
